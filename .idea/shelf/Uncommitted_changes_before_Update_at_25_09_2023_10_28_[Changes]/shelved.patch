+package proyecto.ClasesMER.Usuarios;
+
+import javafx.fxml.FXML;
+import javafx.scene.control.PasswordField;
+import javafx.scene.control.TextField;
+import lombok.AllArgsConstructor;
+import org.springframework.beans.factory.annotation.Autowired;
+import org.springframework.stereotype.Controller;
+
+@Controller
+public class UsuarioController {
+
+    @Autowired
+    private UsuarioService usuarioService;
+
+    @FXML
+    private TextField textFieldSingInScene_Username;
+
+    @FXML
+    private TextField textFieldSingInScene_Nombre;
+
+    @FXML
+    private PasswordField passwordFiedlSingInScene_Password;
+
+    @FXML
+    public void agregarUsuario(){
+        Usuario u = new Usuario();
+        u.setNombre(textFieldSingInScene_Nombre.getText());
+        u.setUsername(textFieldSingInScene_Username.getText());
+        u.setPassword(passwordFiedlSingInScene_Password.getText());
+        u.setIdUsuario(1);
+        usuarioService.agregarUsu(u);
+    }
+}
